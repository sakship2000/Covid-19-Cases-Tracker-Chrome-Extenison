{
    "manifest_version": 2,
    "name": "Covid-19 Cases Tracker",
    "version": "1.0.0",
    "description": "Access the latest Covid-19 cases info for any given country",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon48.png"
    },
    "content_security_policy": "script-src 'self' https://unpkg.com ; object-src 'self'"
  }